<ehcache>
    <cache name="org.hibernate.HibernateFinal.fetchAndEager.Laptop"
           maxEntriesLocalHeap="500"
           eternal="false"
           timeToLiveSeconds="3000"
           timeToIdleSeconds="3000">
    </cache>
    <defaultCache
            maxEntriesLocalHeap="10000"
            eternal="false"
            timeToIdleSeconds="12000"
            timeToLiveSeconds="3000"
            overflowToDisk="false"
            memoryStoreEvictionPolicy="LRU"/>

    <cache name="org.hibernate.HibernateFinal.fetchAndEager.KeyBoard"
           maxEntriesLocalHeap="500"
           eternal="false"
           timeToLiveSeconds="3000"
           timeToIdleSeconds="3000">
    </cache>
</ehcache>
